{
  "name": "multifamily-valuation-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:11100": "PORT=11100 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "setup": "./setup.sh",
    "setup:python": "cd ai_processing && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "test:python": "cd ai_processing && source venv/bin/activate && python3 src/main.py --help",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev-detached": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:prod": "docker-compose up --build",
    "docker:prod-detached": "docker-compose up -d --build",
    "docker:build": "docker-compose build --no-cache",
    "docker:build-dev": "docker-compose -f docker-compose.dev.yml build --no-cache",
    "docker:down": "docker-compose down",
    "docker:down-dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:down-volumes": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "docker:logs-dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker system prune -a --volumes",
    "docker:status": "docker-compose ps",
    "docker:nginx": "docker-compose --profile with-nginx up -d"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^15.4.5",
    "next-auth": "^4.24.7",
    "pptxgenjs": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "readable-stream": "^4.7.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
